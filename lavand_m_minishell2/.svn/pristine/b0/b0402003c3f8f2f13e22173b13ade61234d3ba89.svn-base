void	my_putchar(unsigned char d)
{
  write(1, &d, 1);
}

void	my_putstr(unsigned char *str)
{
  int	i;

  i = 0;
  if (str != 0)
    while (str[i] != 0)
      {
	my_putchar(str[i]);
	i++;
      }
}
