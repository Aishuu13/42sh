
export PS1="\033[05m\033[07m\033[36m[${LOGNAME}] \033[01m${PWD} $ "

alias emacs=emacs -nw
alias ls=ls --color=auto
alias ll=ls -l --color=auto
alias la=ls -A
alias l=ls -l --color=auto -CF
alias ne=emacs -nw
alias clean=rm -f *~; rm -f #*; rm -f *.o
alias c=gcc -W -Wall -Werror -Wextra --std=gnu99
alias g=g++ -W -Wall -Werror -Wextra
alias grun=g++ *.cpp -W -Wall -Werror -Wextra ; ./a.out | cat -e
alias ds=du -sck * | sort -n
alias open=xdg-open
alias sc=screen -S
alias sl=screen -ls
alias sr=screen -r
alias servethis=python -c 'import SimpleHTTPServer; SimpleHTTPServer.test()'
alias sudo=sudocmd
alias cgrep=grep -E -v '^(#|$|;)'
alias nocomment=cgrep
alias bak=bak
alias back=bak
alias niceprod=nicecool
alias np=niceprod
alias extract=extract
alias unall=extract
alias norme=~/norme/norme -nocheat -return -score -libc
alias man=man -Len -a
alias ban=ban
alias q=exit
alias make=make -j8
alias ..=cd ..
alias val=valgrind
alias wotgobblemem=ps -o time,ppid,pid,nice,pcpu,pmem,user,comm -A | sort -n -k 6 | tail -15
alias nautilus=nice -n 10 nautilus .
alias maj=sudo apt-get update && sudo apt-get safe-upgrade
alias c=clear
alias cat=nice -n 15 cat
alias erase=nice -n 16 shred -n 35 -z -u
alias rm=rm --interactive -I --preserve-root --one-file-system --verbose
alias wget=wget -c
alias mv=nice -n 15 mv --verbose
alias cp=nice -n 15 cp --verbose
alias grepr=grep -r
alias grep=grep -i --color
alias mkdir=mkdir -pv
alias t=tree -pxCF -L 2
alias pg=ps aux | grep
alias pl=ps faux | less
alias mkcd=mkcd
alias gti=git
alias gs=git status
alias gd=git diff
alias gc=git commit
alias gl=git log --graph --full-history --all --color
